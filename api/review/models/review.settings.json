{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "name": "Review"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "Rating": {
      "type": "integer",
      "min": 1,
      "max": 10
    },
    "Body": {
      "type": "richtext",
      "required": true
    }
  }
}
